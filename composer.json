{
  "name": "vraimentvraiment/vraimentvraiment.com",
  "description": "Vraiment Vraiment's website",
  "type": "project",
  "keywords": [
    "vraimentvraiment",
    "kirby",
    "cms"
  ],
  "homepage": "https://github.com/VraimentVraiment/vraimentvraiment.com#readme",
  "authors": [
    {
      "name": "Lucas Poulain (ShallowRed)",
      "email": "lucas@vraimentvraiment.com"
    }
  ],
  "support": {
    "email": "lucas@vraimentvraiment.com",
    "issues": "https://github.com/vraimentvraiment/vraimentvraiment.com/issues",
    "source": "https://github.com/vraimentvraiment/vraimentvraiment.com"
  },
  "require": {
    "php": ">=8.3.0",
    "afbora/kirby-loader": "^2.2",
    "afbora/kirby-minify-html": "^2.1",
    "arnoson/kirby-vite": "^5.3.1",
    "getkirby/cms": ">=4.4.1",
    "getkirby/staticache": "^1.0.3",
    "lukaskleinschmidt/kirby-snippet-controller": "^2.2",
    "microman/kirby-column-blocks": "^1.1.1",
    "shallowred/kirby-collection-manager": "^0.1.1",
    "shallowred/kirby-html-sitemap": "^0.1.2",
    "shallowred/kirby-images": "^0.1.1",
    "shallowred/kirby-layouts": "^0.1.3",
    "shallowred/kirby-navigation-menus": "^0.1.5",
    "shallowred/kirby-siblings-pagination": "^0.1.0",
    "shallowred/kirby-social-media": "^0.1.0",
    "shallowred/kirby-tags": "^0.1.0",
    "sylvainjule/matomo": "^2.0.1",
    "tobimori/kirby-seo": "^1.1.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.10.3"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "kirby-cms-path": false,
    "kirby-plugin-path": "storage/plugins"
  }
}
